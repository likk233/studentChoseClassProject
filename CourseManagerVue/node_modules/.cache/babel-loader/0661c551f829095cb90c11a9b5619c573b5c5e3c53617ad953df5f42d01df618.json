   },\n    methods: {\n        query() {\n            api.getPageCount(\n                this.queryForm.courseName,\n                this.queryForm.teacherName\n            ).then((res) => {\n                this.pageCount = res;\n                this.pageIndex = 1;\n                this.getPage(1);\n            });\n        },\n        getPage(pageIndex) {\n            api.getPage(\n                pageIndex,\n                this.queryForm.courseName,\n                this.queryForm.teacherName\n            ).then((res) => {\n                this.tableData = res;\n            });\n        },\n        select(id) {\n            api.select(id).then(() => {\n                this.$message.success(\"选修成功!\");\n                this.getPage(this.pageIndex);\n            });\n        },\n    },\n    created() {\n        this.query();\n\n    },\n    mounted() {\n      \n        OptionApi.getAllowStudentSelect().then((res) => {\n            this.allowSelect = res;\n        });\n    }\n};\n</script>\n\n<style scoped></style>\n"],"mappings":"AA6FA,OAAO,KAAKA,GAAE,MAAO,gCAAgC;AACrD,OAAO,KAAKC,SAAQ,MAAO,kBAAkB;AAE7C,eAAe;EACXC,IAAI,EAAE,qBAAqB;EAC3BC,IAAI,WAAJA,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,SAAS,EAAE;QACPC,UAAU,EAAE,EAAE;QACdC,WAAW,EAAE;MACjB,CAAC;MACDC,SAAS,EAAE,EAAE;MACbC,QAAQ,EAAER,GAAG,CAACQ,QAAQ;MACtBC,SAAS,EAAE,CAAC;MACZC,SAAS,EAAE,CAAC;MACZC,WAAW,EAAE;IACjB,CAAC;EACL,CAAC;EACDC,OAAO,EAAE;IACLC,KAAK,WAALA,KAAKA,CAAA,EAAG;MAAA,IAAAC,KAAA;MACJd,GAAG,CAACe,YAAY,CACZ,IAAI,CAACX,SAAS,CAACC,UAAU,EACzB,IAAI,CAACD,SAAS,CAACE,WACnB,CAAC,CAACU,IAAI,CAAC,UAACC,GAAG,EAAK;QACZH,KAAI,CAACL,SAAQ,GAAIQ,GAAG;QACpBH,KAAI,CAACJ,SAAQ,GAAI,CAAC;QAClBI,KAAI,CAACI,OAAO,CAAC,CAAC,CAAC;MACnB,CAAC,CAAC;IACN,CAAC;IACDA,OAAO,WAAPA,OAAOA,CAACR,SAAS,EAAE;MAAA,IAAAS,MAAA;MACfnB,GAAG,CAACkB,OAAO,CACPR,SAAS,EACT,IAAI,CAACN,SAAS,CAACC,UAAU,EACzB,IAAI,CAACD,SAAS,CAACE,WACnB,CAAC,CAACU,IAAI,CAAC,UAACC,GAAG,EAAK;QACZE,MAAI,CAACZ,SAAQ,GAAIU,GAAG;MACxB,CAAC,CAAC;IACN,CAAC;IACDG,MAAM,WAANA,MAAMA,CAACC,EAAE,EAA