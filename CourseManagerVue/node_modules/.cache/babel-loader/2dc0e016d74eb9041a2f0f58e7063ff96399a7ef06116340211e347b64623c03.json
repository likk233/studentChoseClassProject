inishSave();\n        });\n      }\n    },\n    finishSave: function finishSave() {\n      this.$message.success(\"成功\");\n      this.getPage(this.pageIndex);\n      this.editing = false;\n    },\n    deleteItem: function deleteItem(id) {\n      var _this5 = this;\n      api.deleteItem(id).then(function () {\n        _this5.$message.success(\"删除成功\");\n        _this5.getPage(_this5.pageIndex);\n      });\n    },\n    getCoursesAndStudents: function getCoursesAndStudents() {\n      var _this6 = this;\n      CourseApi.listName().then(function (res) {\n        _this6.courses = res;\n      });\n      StudentApi.listName().then(function (res) {\n        _this6.students = res;\n      });\n    },\n    getSelectAndGradeStatus: function getSelectAndGradeStatus() {\n      var _this7 = this;\n      OptionApi.getAllowStudentSelect().then(function (res) {\n        _this7.optionForm.allowSelect = res;\n      });\n      OptionApi.getAllowTeacherGrade().then(function (res) {\n        _this7.optionForm.allowGrade = res;\n      });\n    },\n    updateAllowSelect: function updateAllowSelect(val) {\n      OptionApi.setAllowStudentSelect(val);\n    },\n    updateAllowGrade: function updateAllowGrade(val) {\n      OptionApi.setAllowTeacherGrade(val);\n    }\n  },\n  created: function created() {\n    this.query();\n    this.getCoursesAndStudents();\n    this.getSelectAndGradeStatus();\n  }\n};","map":{"version":3,"names":["api","CourseApi","StudentApi","OptionApi","name","data","queryForm","className","courseName","studentName","optionForm","allowSelect","allowGrade","entityForm","tableData","pageSize","pageCount","pageIndex","editing","courses","students","methods","query","_this","getPageCount","then","res","getPage","_this2","create","id","studentId","courseId","dailyScore","examScore","score","edit","_this3","get","save","_this4","finishSave","update","$message","success","deleteItem","_this5","getCoursesAndStudents","_this6","listName","getSelectAndGradeStatus","_this7","getAllowStu