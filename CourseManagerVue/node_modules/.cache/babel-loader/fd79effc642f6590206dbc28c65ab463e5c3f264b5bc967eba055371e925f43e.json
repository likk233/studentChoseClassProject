{"ast":null,"code":"import * as api from \"../../api/admin/course\";\nimport * as teacherApi from \"../../api/admin/teacher\";\nexport default {\n  name: \"AdminCourse\",\n  data() {\n    return {\n      queryForm: {\n        departmentName: \"\",\n        teacherName: \"\",\n        courseName: \"\"\n      },\n      entityForm: {},\n      tableData: [],\n      pageSize: api.pageSize,\n      pageCount: 1,\n      pageIndex: 1,\n      editing: false,\n      teachers: [],\n      courseDay: \"\",\n      courseTime: \"\",\n      courseLength: 0,\n      days: [\"星期一\", \"星期二\", \"星期三\", \"星期四\", \"星期五\", \"星期六\", \"星期日\"],\n      times: [\"第一节\", \"第二节\", \"第三节\", \"第四节\", \"第五节\", \"第六节\", \"第七节\", \"第八节\", \"第九节\"]\n    };\n  },\n  methods: {\n    query() {\n      api.getPageCount(this.queryForm.departmentName, this.queryForm.teacherName, this.queryForm.courseName).then(res => {\n        this.pageCount = res;\n        this.pageIndex = 1;\n        this.getPage(1);\n      });\n    },\n    getPage(pageIndex) {\n      api.getPage(pageIndex, this.queryForm.departmentName, this.queryForm.teacherName, this.queryForm.courseName).then(res => {\n        this.tableData = res;\n      });\n    },\n    create() {\n      this.entityForm = {\n        id: -1,\n        teacherId: null,\n        name: \"\",\n        grade: 2023,\n        time: \"\",\n        location: \"\",\n        credit: 2,\n        maxSize: 50,\n        examDate: null,\n        examLocation: null\n      };\n      this.courseDay = 1;\n      this.courseTime = 1;\n      this.courseLength = 2;\n      this.editing = true;\n    },\n    edit(id) {\n      api.get(id).then(res => {\n        let split = res.time.split(\"-\");\n        this.courseDay = parseInt(split[0]) - 1;\n        this.courseTime = parseInt(split[1]) - 1;\n        this.courseLength = parseInt(split[2]);\n        this.entityForm = res;\n        this.editing = true;\n      });\n    },\n    save() {\n      let day = this.courseDay + 1;\n      let time = this.courseTime + 1;\n      this.entityForm.time = day + \"-\" + time + \"-\" + this.courseLength;\n      if (this.entityForm.id === -1) {\n        api.create(this.entityForm).then(() => {\n          this.finishSave();\n        });\n      } else {\n        api.update(this.entityForm).then(() => {\n          this.finishSave();\n        });\n      }\n    },\n    finishSave() {\n      this.$message.success(\"成功\");\n      this.getPage(this.pageIndex);\n      this.editing = false;\n    },\n    deleteItem(id) {\n      api.deleteItem(id).then(() => {\n        this.$message.success(\"删除成功\");\n        this.getPage(this.pageIndex);\n      });\n    },\n    getTeachers() {\n      teacherApi.listName().then(res => {\n        this.teachers = res;\n      });\n    }\n  },\n  created() {\n    this.query();\n    this.getTeachers();\n  }\n};","map":{"version":3,"names":["api","teacherApi","name","data","queryForm","departmentName","teacherName","courseName","entityForm","tableData","pageSize","pageCount","pageIndex","editing","teachers","courseDay","courseTime","courseLength","days","times","methods","query","getPageCount","then","res","getPage","create","id","teacherId","grade","time","location","credit","maxSize","examDate","examLocation","edit","get","split","parseInt","save","day","finishSave","update","$message","success","deleteItem","getTeachers","listName","created"],"sources":["/Users/lizi/Documents/GitHub/lib/studentChoseClassProject/CourseManagerVue/src/views/admin/AdminCourse.vue"],"sourcesContent":["<template>\n    <div class=\"course-wrap\">\n        <div class=\"crumbs\">\n            <el-breadcrumb separator=\"/\">\n                <el-breadcrumb-item>\n                    <b style=\"font-size: 22px\"> 课程管理</b>\n                </el-breadcrumb-item>\n            </el-breadcrumb>\n        </div>\n\n        <div class=\"container\">\n            <div class=\"query-form\">\n                <el-row :gutter=\"20\">\n                    <el-col :span=\"2\">\n                        <el-button @click=\"create\" style=\"font-size: 18px\"\n                            >\n                            创建</el-button\n                        >\n                    </el-col>\n                    <el-col :offset=\"10\" :span=\"3\">\n                        <el-input\n                            @keyup.enter.native=\"query\"\n                            placeholder=\"课程名\"\n                            v-model=\"queryForm.courseName\"\n                        />\n                    </el-col>\n                    <el-col :span=\"3\">\n                        <el-input\n                            @keyup.enter.native=\"query\"\n                            placeholder=\"教师名\"\n                            v-model=\"queryForm.teacherName\"\n                        />\n                    </el-col>\n                    <el-col :span=\"3\">\n                        <el-input\n                            @keyup.enter.native=\"query\"\n                            placeholder=\"系名\"\n                            v-model=\"queryForm.departmentName\"\n                        />\n                    </el-col>\n                    <el-col :span=\"3\">\n                        <el-button\n                            @click=\"query\"\n                            type=\"primary\"\n                            style=\"font-size: 18px\"\n                        >\n                            搜索\n                        </el-button>\n                    </el-col>\n                </el-row>\n            </div>\n\n            <div class=\"table\">\n                <el-table :data=\"tableData\" stripe>\n                    <el-table-column label=\"课程Id\" prop=\"id\" />\n                    <el-table-column label=\"课程名\" prop=\"name\" width=\"200px\" />\n                    <el-table-column label=\"教师\" prop=\"teacherName\" />\n                    <el-table-column label=\"所属系\" prop=\"departmentName\" />\n                    <el-table-column label=\"年级\" prop=\"grade\" />\n                    <el-table-column label=\"学分\" prop=\"credit\" />\n                    <el-table-column\n                        align=\"center\"\n                        label=\"上课时间\"\n                        prop=\"time\"\n                        width=\"130px\"\n                    />\n                    <el-table-column label=\"已选人数\" prop=\"selectedCount\" />\n                    <el-table-column label=\"最大容量\" prop=\"maxSize\" />\n                    <el-table-column\n                        align=\"center\"\n                        label=\"操作\"\n                        width=\"250px\"\n                        fixed=\"right\"\n                    >\n                        <template slot-scope=\"scope\">\n                            <el-button\n                                @click=\"edit(scope.row.id)\"\n                                type=\"success\"\n                                style=\"font-size: 18px\"\n                            >\n                                编辑\n                            </el-button>\n                            <el-button\n                                @click=\"deleteItem(scope.row.id)\"\n                                type=\"danger\"\n                                style=\"font-size: 18px\"\n                            >\n                                删除\n                            </el-button>\n                        </template>\n                    </el-table-column>\n                </el-table>\n            </div>\n\n            <el-row justify=\"center\" type=\"flex\" style=\"margin: 5px 0 5px 0\">\n                <el-pagination\n                    :current-page.sync=\"pageIndex\"\n                    :page-size=\"pageSize\"\n                    :total=\"pageSize * pageCount\"\n                    @current-change=\"getPage\"\n                    background\n                    layout=\"prev, pager, next\"\n                >\n                </el-pagination>\n            </el-row>\n\n            <el-dialog :visible.sync=\"editing\" title=\"编辑\" width=\"30%\">\n                <el-form :model=\"entityForm\" label-width=\"70px\" ref=\"form\">\n                    <el-form-item label=\"课程名\">\n                        <el-input v-model=\"entityForm.name\"></el-input>\n                    </el-form-item>\n                    <el-form-item label=\"授课教师\">\n                        <el-select\n                            placeholder=\"请选择教师\"\n                            v-model=\"entityForm.teacherId\"\n                        >\n                            <el-option\n                                :key=\"index\"\n                                :label=\"item.name\"\n                                :value=\"item.id\"\n                                v-for=\"(item, index) in teachers\"\n                            />\n                        </el-select>\n                    </el-form-item>\n                    <el-form-item label=\"年级\">\n                        <el-input\n                            type=\"number\"\n                            v-model=\"entityForm.grade\"\n                        ></el-input>\n                    </el-form-item>\n                    <el-form-item label=\"上课时间\">\n                        <el-select v-model=\"courseDay\">\n                            <el-option\n                                :key=\"index\"\n                                :label=\"item\"\n                                :value=\"index\"\n                                v-for=\"(item, index) in days\"\n                            >\n                            </el-option>\n                        </el-select>\n                        <el-select v-model=\"courseTime\">\n                            <el-option\n                                :key=\"index\"\n                                :label=\"item\"\n                                :value=\"index\"\n                                v-for=\"(item, index) in times\"\n                            >\n                            </el-option>\n                        </el-select>\n                    </el-form-item>\n                    <el-form-item label=\"时长(节)\">\n                        <el-input\n                            type=\"number\"\n                            v-model=\"courseLength\"\n                        ></el-input>\n                    </el-form-item>\n                    <el-form-item label=\"上课地点\">\n                        <el-input v-model=\"entityForm.location\"></el-input>\n                    </el-form-item>\n                    <el-form-item label=\"学分\">\n                        <el-input\n                            type=\"number\"\n                            v-model=\"entityForm.credit\"\n                        ></el-input>\n                    </el-form-item>\n                    <el-form-item label=\"最大容量\">\n                        <el-input\n                            type=\"number\"\n                            v-model=\"entityForm.maxSize\"\n                        ></el-input>\n                    </el-form-item>\n                    <!--<el-form-item label=\"考试时间\">-->\n                    <!--<el-date-picker-->\n                    <!--format=\"yyyy-MM-dd HH:mm\"-->\n                    <!--type=\"datetime\"-->\n                    <!--v-model=\"entityForm.examDate\"-->\n                    <!--value-format=\"yyyy-MM-dd HH:mm\"-->\n                    <!--&gt;-->\n                    <!--</el-date-picker>-->\n                    <!--</el-form-item>-->\n                    <el-form-item label=\"考试地点\">\n                        <el-input v-model=\"entityForm.examLocation\"></el-input>\n                    </el-form-item>\n                </el-form>\n                <span class=\"dialog-footer\" slot=\"footer\">\n                    <el-button\n                        @click=\"save\"\n                        style=\"font-size: 18px\"\n                        type=\"primary\"\n                        >\n                        确 定</el-button\n                    >\n                    <el-button @click=\"editing = false\" style=\"font-size: 18px\"\n                        >\n                        取 消</el-button\n                    >\n                </span>\n            </el-dialog>\n        </div>\n    </div>\n</template>\n\n<script>\nimport * as api from \"../../api/admin/course\";\nimport * as teacherApi from \"../../api/admin/teacher\";\n\nexport default {\n    name: \"AdminCourse\",\n    data() {\n        return {\n            queryForm: {\n                departmentName: \"\",\n                teacherName: \"\",\n                courseName: \"\",\n            },\n            entityForm: {},\n            tableData: [],\n            pageSize: api.pageSize,\n            pageCount: 1,\n            pageIndex: 1,\n            editing: false,\n            teachers: [],\n            courseDay: \"\",\n            courseTime: \"\",\n            courseLength: 0,\n            days: [\n                \"星期一\",\n                \"星期二\",\n                \"星期三\",\n                \"星期四\",\n                \"星期五\",\n                \"星期六\",\n                \"星期日\",\n            ],\n            times: [\n                \"第一节\",\n                \"第二节\",\n                \"第三节\",\n                \"第四节\",\n                \"第五节\",\n                \"第六节\",\n                \"第七节\",\n                \"第八节\",\n                \"第九节\",\n            ],\n        };\n    },\n    methods: {\n        query() {\n            api.getPageCount(\n                this.queryForm.departmentName,\n                this.queryForm.teacherName,\n                this.queryForm.courseName\n            ).then((res) => {\n                this.pageCount = res;\n                this.pageIndex = 1;\n                this.getPage(1);\n            });\n        },\n        getPage(pageIndex) {\n            api.getPage(\n                pageIndex,\n                this.queryForm.departmentName,\n                this.queryForm.teacherName,\n                this.queryForm.courseName\n            ).then((res) => {\n                this.tableData = res;\n            });\n        },\n        create() {\n            this.entityForm = {\n                id: -1,\n                teacherId: null,\n                name: \"\",\n                grade: 2023,\n                time: \"\",\n                location: \"\",\n                credit: 2,\n                maxSize: 50,\n                examDate: null,\n                examLocation: null,\n            };\n            this.courseDay = 1;\n            this.courseTime = 1;\n            this.courseLength = 2;\n            this.editing = true;\n        },\n        edit(id) {\n            api.get(id).then((res) => {\n                let split = res.time.split(\"-\");\n                this.courseDay = parseInt(split[0]) - 1;\n                this.courseTime = parseInt(split[1]) - 1;\n                this.courseLength = parseInt(split[2]);\n                this.entityForm = res;\n                this.editing = true;\n            });\n        },\n        save() {\n            let day = this.courseDay + 1;\n            let time = this.courseTime + 1;\n            this.entityForm.time = day + \"-\" + time + \"-\" + this.courseLength;\n\n            if (this.entityForm.id === -1) {\n                api.create(this.entityForm).then(() => {\n                    this.finishSave();\n                });\n            } else {\n                api.update(this.entityForm).then(() => {\n                    this.finishSave();\n                });\n            }\n        },\n        finishSave() {\n            this.$message.success(\"成功\");\n            this.getPage(this.pageIndex);\n            this.editing = false;\n        },\n        deleteItem(id) {\n            api.deleteItem(id).then(() => {\n                this.$message.success(\"删除成功\");\n                this.getPage(this.pageIndex);\n            });\n        },\n        getTeachers() {\n            teacherApi.listName().then((res) => {\n                this.teachers = res;\n            });\n        },\n    },\n    created() {\n        this.query();\n        this.getTeachers();\n    },\n};\n</script>\n\n<style scoped></style>\n"],"mappings":"AA2MA,OAAO,KAAKA,GAAE,MAAO,wBAAwB;AAC7C,OAAO,KAAKC,UAAS,MAAO,yBAAyB;AAErD,eAAe;EACXC,IAAI,EAAE,aAAa;EACnBC,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,SAAS,EAAE;QACPC,cAAc,EAAE,EAAE;QAClBC,WAAW,EAAE,EAAE;QACfC,UAAU,EAAE;MAChB,CAAC;MACDC,UAAU,EAAE,CAAC,CAAC;MACdC,SAAS,EAAE,EAAE;MACbC,QAAQ,EAAEV,GAAG,CAACU,QAAQ;MACtBC,SAAS,EAAE,CAAC;MACZC,SAAS,EAAE,CAAC;MACZC,OAAO,EAAE,KAAK;MACdC,QAAQ,EAAE,EAAE;MACZC,SAAS,EAAE,EAAE;MACbC,UAAU,EAAE,EAAE;MACdC,YAAY,EAAE,CAAC;MACfC,IAAI,EAAE,CACF,KAAK,EACL,KAAK,EACL,KAAK,EACL,KAAK,EACL,KAAK,EACL,KAAK,EACL,KAAK,CACR;MACDC,KAAK,EAAE,CACH,KAAK,EACL,KAAK,EACL,KAAK,EACL,KAAK,EACL,KAAK,EACL,KAAK,EACL,KAAK,EACL,KAAK,EACL,KAAK;IAEb,CAAC;EACL,CAAC;EACDC,OAAO,EAAE;IACLC,KAAKA,CAAA,EAAG;MACJrB,GAAG,CAACsB,YAAY,CACZ,IAAI,CAAClB,SAAS,CAACC,cAAc,EAC7B,IAAI,CAACD,SAAS,CAACE,WAAW,EAC1B,IAAI,CAACF,SAAS,CAACG,UACnB,CAAC,CAACgB,IAAI,CAAEC,GAAG,IAAK;QACZ,IAAI,CAACb,SAAQ,GAAIa,GAAG;QACpB,IAAI,CAACZ,SAAQ,GAAI,CAAC;QAClB,IAAI,CAACa,OAAO,CAAC,CAAC,CAAC;MACnB,CAAC,CAAC;IACN,CAAC;IACDA,OAAOA,CAACb,SAAS,EAAE;MACfZ,GAAG,CAACyB,OAAO,CACPb,SAAS,EACT,IAAI,CAACR,SAAS,CAACC,cAAc,EAC7B,IAAI,CAACD,SAAS,CAACE,WAAW,EAC1B,IAAI,CAACF,SAAS,CAACG,UACnB,CAAC,CAACgB,IAAI,CAAEC,GAAG,IAAK;QACZ,IAAI,CAACf,SAAQ,GAAIe,GAAG;MACxB,CAAC,CAAC;IACN,CAAC;IACDE,MAAMA,CAAA,EAAG;MACL,IAAI,CAAClB,UAAS,GAAI;QACdmB,EAAE,EAAE,CAAC,CAAC;QACNC,SAAS,EAAE,IAAI;QACf1B,IAAI,EAAE,EAAE;QACR2B,KAAK,EAAE,IAAI;QACXC,IAAI,EAAE,EAAE;QACRC,QAAQ,EAAE,EAAE;QACZC,MAAM,EAAE,CAAC;QACTC,OAAO,EAAE,EAAE;QACXC,QAAQ,EAAE,IAAI;QACdC,YAAY,EAAE;MAClB,CAAC;MACD,IAAI,CAACpB,SAAQ,GAAI,CAAC;MAClB,IAAI,CAACC,UAAS,GAAI,CAAC;MACnB,IAAI,CAACC,YAAW,GAAI,CAAC;MACrB,IAAI,CAACJ,OAAM,GAAI,IAAI;IACvB,CAAC;IACDuB,IAAIA,CAACT,EAAE,EAAE;MACL3B,GAAG,CAACqC,GAAG,CAACV,EAAE,CAAC,CAACJ,IAAI,CAAEC,GAAG,IAAK;QACtB,IAAIc,KAAI,GAAId,GAAG,CAACM,IAAI