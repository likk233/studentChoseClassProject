{"ast":null,"code":"import * as api from \"../../api/admin/studentCourse\";\nimport * as CourseApi from \"../../api/admin/course\";\nimport * as StudentApi from \"../../api/admin/student\";\nimport * as OptionApi from \"../../api/option\";\nexport default {\n  name: \"AdminStudentCourse\",\n  data() {\n    return {\n      queryForm: {\n        className: \"\",\n        courseName: \"\",\n        studentName: \"\"\n      },\n      optionForm: {\n        allowSelect: true,\n        allowGrade: true\n      },\n      entityForm: {},\n      tableData: [],\n      pageSize: api.pageSize,\n      pageCount: 1,\n      pageIndex: 1,\n      editing: false,\n      courses: [],\n      students: []\n    };\n  },\n  methods: {\n    query() {\n      api.getPageCount(this.queryForm.className, this.queryForm.courseName, this.queryForm.studentName).then(res => {\n        this.pageCount = res;\n        this.pageIndex = 1;\n        this.getPage(1);\n      });\n    },\n    getPage(pageIndex) {\n      api.getPage(pageIndex, this.queryForm.className, this.queryForm.courseName, this.queryForm.studentName).then(res => {\n        this.tableData = res;\n      });\n    },\n    create() {\n      this.entityForm = {\n        id: -1,\n        studentId: null,\n        courseId: null,\n        dailyScore: null,\n        examScore: null,\n        score: null\n      };\n      this.editing = true;\n    },\n    edit(id) {\n      api.get(id).then(res => {\n        this.entityForm = res;\n        this.editing = true;\n      });\n    },\n    save() {\n      if (this.entityForm.id === -1) {\n        api.create(this.entityForm).then(() => {\n          this.finishSave();\n        });\n      } else {\n        api.update(this.entityForm).then(() => {\n          this.finishSave();\n        });\n      }\n    },\n    finishSave() {\n      this.$message.success(\"成功\");\n      this.getPage(this.pageIndex);\n      this.editing = false;\n    },\n    deleteItem(id) {\n      api.deleteItem(id).then(() => {\n        this.$message.success(\"删除成功\");\n        this.getPage(this.pageIndex);\n      });\n    },\n    getCoursesAndStudents() {\n      CourseApi.listName().then(res => {\n        this.courses = res;\n      });\n      StudentApi.listName().then(res => {\n        this.students = res;\n      });\n    },\n    getSelectAndGradeStatus() {\n      OptionApi.getAllowStudentSelect().then(res => {\n        this.optionForm.allowSelect = res;\n      });\n      OptionApi.getAllowTeacherGrade().then(res => {\n        this.optionForm.allowGrade = res;\n      });\n    },\n    updateAllowSelect(val) {\n      OptionApi.setAllowStudentSelect(val);\n    },\n    updateAllowGrade(val) {\n      OptionApi.setAllowTeacherGrade(val);\n    }\n  },\n  created() {\n    this.query();\n    this.getCoursesAndStudents();\n    this.getSelectAndGradeStatus();\n  }\n};","map":{"version":3,"names":["api","CourseApi","StudentApi","OptionApi","name","data","queryForm","className","courseName","studentName","optionForm","allowSelect","allowGrade","entityForm","tableData","pageSize","pageCount","pageIndex","editing","courses","students","methods","query","getPageCount","then","res","getPage","create","id","studentId","courseId","dailyScore","examScore","score","edit","get","save","finishSave","update","$message","success","deleteItem","getCoursesAndStudents","listName","getSelectAndGradeStatus","getAllowStudentSelect","getAllowTeacherGrade","updateAllowSelect","val","setAllowStudentSelect","updateAllowGrade","setAllowTeacherGrade","created"],"sources":["/Users/lizi/Documents/GitHub/lib/studentChoseClassProject/CourseManagerVue/src/views/admin/AdminStudentCourse.vue"],"sourcesContent":["<template>\n    <div class=\"student-course-wrap\">\n        <div class=\"crumbs\">\n            <el-breadcrumb separator=\"/\">\n                <el-breadcrumb-item>\n                    <b style=\"font-size: 22px\"> 选课管理</b>\n                </el-breadcrumb-item>\n            </el-breadcrumb>\n        </div>\n\n        <div class=\"container\">\n            <div class=\"query-form\">\n                <el-row :gutter=\"20\">\n                    <el-col :span=\"3\">\n                        <el-button @click=\"create\" style=\"font-size: 18px\"\n                            >\n                            创建</el-button\n                        >\n                    </el-col>\n                    <el-col :span=\"4\">\n                        <el-checkbox\n                            @change=\"updateAllowSelect\"\n                            v-model=\"optionForm.allowSelect\"\n                            >启用学生选课功能\n                        </el-checkbox>\n                        <el-checkbox\n                            @change=\"updateAllowGrade\"\n                            v-model=\"optionForm.allowGrade\"\n                            >启用教师评分功能\n                        </el-checkbox>\n                    </el-col>\n                    <el-col :offset=\"4\" :span=\"3\">\n                        <el-input\n                            @keyup.enter.native=\"query\"\n                            placeholder=\"课程名\"\n                            v-model=\"queryForm.courseName\"\n                        />\n                    </el-col>\n                    <el-col :span=\"3\">\n                        <el-input\n                            @keyup.enter.native=\"query\"\n                            placeholder=\"学生名\"\n                            v-model=\"queryForm.studentName\"\n                        />\n                    </el-col>\n                    <el-col :span=\"3\">\n                        <el-input\n                            @keyup.enter.native=\"query\"\n                            placeholder=\"班级名\"\n                            v-model=\"queryForm.className\"\n                        />\n                    </el-col>\n                    <el-col :span=\"3\">\n                        <el-button\n                            @click=\"query\"\n                            type=\"primary\"\n                            style=\"font-size: 18px\"\n                        >\n                            搜索\n                        </el-button>\n                    </el-col>\n                </el-row>\n            </div>\n\n            <div class=\"table\">\n                <el-table :data=\"tableData\" stripe>\n                    <el-table-column label=\"选课Id\" prop=\"id\" width=\"80px\" />\n                    <el-table-column label=\"课程名\" prop=\"courseName\" />\n                    <el-table-column label=\"学生名\" prop=\"studentName\" />\n                    <el-table-column label=\"学生班级\" prop=\"className\" />\n                    <el-table-column\n                        label=\"日常分\"\n                        min-width=\"80px\"\n                        prop=\"dailyScore\"\n                    />\n                    <el-table-column\n                        label=\"期末分\"\n                        prop=\"examScore\"\n                        width=\"80px\"\n                    />\n                    <el-table-column label=\"总分\" prop=\"score\" width=\"80px\" />\n                    <el-table-column\n                        align=\"center\"\n                        label=\"操作\"\n                        width=\"250px\"\n                        fixed=\"right\"\n                    >\n                        <template slot-scope=\"scope\">\n                            <el-button\n                                @click=\"edit(scope.row.id)\"\n                                type=\"success\"\n                                style=\"font-size: 18px\"\n                            >\n                                编辑\n                            </el-button>\n                            <el-button\n                                @click=\"deleteItem(scope.row.id)\"\n                                type=\"danger\"\n                                style=\"font-size: 18px\"\n                            >\n                                删除\n                            </el-button>\n                        </template>\n                    </el-table-column>\n                </el-table>\n            </div>\n\n            <el-row justify=\"center\" type=\"flex\" style=\"margin: 5px 0 5px 0\">\n                <el-pagination\n                    :current-page.sync=\"pageIndex\"\n                    :page-size=\"pageSize\"\n                    :total=\"pageSize * pageCount\"\n                    @current-change=\"getPage\"\n                    background\n                    layout=\"prev, pager, next\"\n                >\n                </el-pagination>\n            </el-row>\n\n            <el-dialog :visible.sync=\"editing\" title=\"编辑\" width=\"30%\">\n                <el-form :model=\"entityForm\" label-width=\"70px\" ref=\"form\">\n                    <el-form-item label=\"学生\">\n                        <el-select\n                            :disabled=\"entityForm.id !== -1\"\n                            placeholder=\"请选择学生\"\n                            v-model=\"entityForm.studentId\"\n                        >\n                            <el-option\n                                :key=\"index\"\n                                :label=\"item.name\"\n                                :value=\"item.id\"\n                                v-for=\"(item, index) in students\"\n                            />\n                        </el-select>\n                    </el-form-item>\n                    <el-form-item label=\"课程\">\n                        <el-select\n                            :disabled=\"entityForm.id !== -1\"\n                            placeholder=\"请选择课程\"\n                            v-model=\"entityForm.courseId\"\n                        >\n                            <el-option\n                                :key=\"index\"\n                                :label=\"item.name\"\n                                :value=\"item.id\"\n                                v-for=\"(item, index) in courses\"\n                            />\n                        </el-select>\n                    </el-form-item>\n                    <el-form-item label=\"日常得分\">\n                        <el-input\n                            type=\"number\"\n                            v-model=\"entityForm.dailyScore\"\n                        ></el-input>\n                    </el-form-item>\n                    <el-form-item label=\"考试得分\">\n                        <el-input\n                            type=\"number\"\n                            v-model=\"entityForm.examScore\"\n                        ></el-input>\n                    </el-form-item>\n                    <el-form-item label=\"总分\">\n                        <el-input\n                            type=\"number\"\n                            v-model=\"entityForm.score\"\n                        ></el-input>\n                    </el-form-item>\n                </el-form>\n                <span class=\"dialog-footer\" slot=\"footer\">\n                    <el-button\n                        @click=\"save\"\n                        type=\"primary\"\n                        style=\"font-size: 18px\"\n                        >\n                        确 定</el-button\n                    >\n                    <el-button @click=\"editing = false\" style=\"font-size: 18px\"\n                        >\n                        取 消</el-button\n                    >\n                </span>\n            </el-dialog>\n        </div>\n    </div>\n</template>\n\n<script>\nimport * as api from \"../../api/admin/studentCourse\";\nimport * as CourseApi from \"../../api/admin/course\";\nimport * as StudentApi from \"../../api/admin/student\";\nimport * as OptionApi from \"../../api/option\";\n\nexport default {\n    name: \"AdminStudentCourse\",\n    data() {\n        return {\n            queryForm: {\n                className: \"\",\n                courseName: \"\",\n                studentName: \"\",\n            },\n            optionForm: {\n                allowSelect: true,\n                allowGrade: true,\n            },\n            entityForm: {},\n            tableData: [],\n            pageSize: api.pageSize,\n            pageCount: 1,\n            pageIndex: 1,\n            editing: false,\n            courses: [],\n            students: [],\n        };\n    },\n    methods: {\n        query() {\n            api.getPageCount(\n                this.queryForm.className,\n                this.queryForm.courseName,\n                this.queryForm.studentName\n            ).then((res) => {\n                this.pageCount = res;\n                this.pageIndex = 1;\n                this.getPage(1);\n            });\n        },\n        getPage(pageIndex) {\n            api.getPage(\n                pageIndex,\n                this.queryForm.className,\n                this.queryForm.courseName,\n                this.queryForm.studentName\n            ).then((res) => {\n                this.tableData = res;\n            });\n        },\n        create() {\n            this.entityForm = {\n                id: -1,\n                studentId: null,\n                courseId: null,\n                dailyScore: null,\n                examScore: null,\n                score: null,\n            };\n            this.editing = true;\n        },\n        edit(id) {\n            api.get(id).then((res) => {\n                this.entityForm = res;\n                this.editing = true;\n            });\n        },\n        save() {\n            if (this.entityForm.id === -1) {\n                api.create(this.entityForm).then(() => {\n                    this.finishSave();\n                });\n            } else {\n                api.update(this.entityForm).then(() => {\n                    this.finishSave();\n                });\n            }\n        },\n        finishSave() {\n            this.$message.success(\"成功\");\n            this.getPage(this.pageIndex);\n            this.editing = false;\n        },\n        deleteItem(id) {\n            api.deleteItem(id).then(() => {\n                this.$message.success(\"删除成功\");\n                this.getPage(this.pageIndex);\n            });\n        },\n        getCoursesAndStudents() {\n            CourseApi.listName().then((res) => {\n                this.courses = res;\n            });\n            StudentApi.listName().then((res) => {\n                this.students = res;\n            });\n        },\n        getSelectAndGradeStatus() {\n            OptionApi.getAllowStudentSelect().then((res) => {\n                this.optionForm.allowSelect = res;\n            });\n            OptionApi.getAllowTeacherGrade().then((res) => {\n                this.optionForm.allowGrade = res;\n            });\n        },\n        updateAllowSelect(val) {\n            OptionApi.setAllowStudentSelect(val);\n        },\n        updateAllowGrade(val) {\n            OptionApi.setAllowTeacherGrade(val);\n        },\n    },\n    created() {\n        this.query();\n        this.getCoursesAndStudents();\n        this.getSelectAndGradeStatus();\n    },\n};\n</script>\n\n<style scoped></style>\n"],"mappings":"AA2LA,OAAO,KAAKA,GAAE,MAAO,+BAA+B;AACpD,OAAO,KAAKC,SAAQ,MAAO,wBAAwB;AACnD,OAAO,KAAKC,UAAS,MAAO,yBAAyB;AACrD,OAAO,KAAKC,SAAQ,MAAO,kBAAkB;AAE7C,eAAe;EACXC,IAAI,EAAE,oBAAoB;EAC1BC,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,SAAS,EAAE;QACPC,SAAS,EAAE,EAAE;QACbC,UAAU,EAAE,EAAE;QACdC,WAAW,EAAE;MACjB,CAAC;MACDC,UAAU,EAAE;QACRC,WAAW,EAAE,IAAI;QACjBC,UAAU,EAAE;MAChB,CAAC;MACDC,UAAU,EAAE,CAAC,CAAC;MACdC,SAAS,EAAE,EAAE;MACbC,QAAQ,EAAEf,GAAG,CAACe,QAAQ;MACtBC,SAAS,EAAE,CAAC;MACZC,SAAS,EAAE,CAAC;MACZC,OAAO,EAAE,KAAK;MACdC,OAAO,EAAE,EAAE;MACXC,QAAQ,EAAE;IACd,CAAC;EACL,CAAC;EACDC,OAAO,EAAE;IACLC,KAAKA,CAAA,EAAG;MACJtB,GAAG,CAACuB,YAAY,CACZ,IAAI,CAACjB,SAAS,CAACC,SAAS,EACxB,IAAI,CAACD,SAAS,CAACE,UAAU,EACzB,IAAI,CAACF,SAAS,CAACG,WACnB,CAAC,CAACe,IAAI,CAAEC,GAAG,IAAK;QACZ,IAAI,CAACT,SAAQ,GAAIS,GAAG;QACpB,IAAI,CAACR,SAAQ,GAAI,CAAC;QAClB,IAAI,CAACS,OAAO,CAAC,C