},\n        create() {\n            this.entityForm = {\n                id: -1,\n                name: \"\",\n                grade: 2023,\n                majorId: \"\",\n            };\n            this.editing = true;\n        },\n        edit(id) {\n            api.get(id).then((res) => {\n                this.entityForm = res;\n                this.editing = true;\n            });\n        },\n        save() {\n            if (this.entityForm.id === -1) {\n                api.create(this.entityForm).then(() => {\n                    this.finishSave();\n                });\n            } else {\n                api.update(this.entityForm).then(() => {\n                    this.finishSave();\n                });\n            }\n        },\n        finishSave() {\n            this.$message.success(\"成功\");\n            this.getPage(this.pageIndex);\n            this.editing = false;\n        },\n        deleteItem(id) {\n            api.deleteItem(id).then(() => {\n                this.$message.success(\"删除成功\");\n                this.getPage(this.pageIndex);\n            });\n        },\n        getMajors() {\n            majorApi.listName().then((res) => {\n                this.majors = res;\n            });\n        },\n    },\n    created() {\n        this.query();\n        this.getMajors();\n    },\n};\n</script>\n\n<style scoped></style>\n"],"mappings":";AA6IA,OAAO,KAAKA,GAAE,MAAO,uBAAuB;AAC5C,OAAO,KAAKC,QAAO,MAAO,uBAAuB;AAEjD,eAAe;EACXC,IAAI,EAAE,YAAY;EAClBC,IAAI,WAAJA,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,SAAS,EAAE;QACPC,cAAc,EAAE,EAAE;QAClBC,SAAS,EAAE,EAAE;QACbJ,IAAI,EAAE;MACV,CAAC;MACDK,UAAU,EAAE,CAAC,CAAC;MACdC,SAAS,EAAE,EAAE;MACbC,QAAQ,EAAET,GAAG,CAACS,QAAQ;MACtBC,SAAS,EAAE,CAAC;MACZC,SAAS,EAAE,CAAC;MACZC,OAAO,EAAE,KAAK;MACdC,MAAM,EAAE;IACZ,CAAC;EACL,CAAC;EACDC,OAAO,EAAE;IACLC,KAAK,WAALA,KAAKA,CAAA,EAAG;MAAA,IAAAC,KAAA;MACJhB,GAAG,CAACiB,YAAY,CACZ,IAAI,CAACb,SAAS,CAACC,cAAc,EAC7B,IAAI,CAACD,SAAS,CAACE,SAAS,EACxB,IAAI,CAACF,SAAS,CAACF,IACnB,CAAC,CAACgB,IAAI,