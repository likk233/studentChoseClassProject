nput\n                            @keyup.enter.native=\"query\"\n                            placeholder=\"学生姓名\"\n                            v-model=\"queryForm.name\"\n                        />\n                    </el-col>\n                    <el-col :span=\"3\">\n                        <el-input\n                            @keyup.enter.native=\"query\"\n                            placeholder=\"专业名\"\n                            v-model=\"queryForm.majorName\"\n                        />\n                    </el-col>\n                    <el-col :span=\"3\">\n                        <el-input\n                            @keyup.enter.native=\"query\"\n                            placeholder=\"班级名\"\n                            v-model=\"queryForm.className\"\n                        />\n                    </el-col>\n                    <el-col :span=\"3\">\n                        <el-button\n                            @click=\"query\"\n                            type=\"primary\"\n                            style=\"font-size: 18px\"\n                        >\n                            搜索\n                        </el-button>\n                    </el-col>\n                </el-row>\n            </div>\n\n            <div class=\"table\">\n                <el-table :data=\"tableData\" stripe>\n                    <el-table-column label=\"学生Id\" prop=\"id\" width=\"80px\" />\n                    <el-table-column label=\"学号\" prop=\"number\" />\n                    <el-table-column label=\"姓名\" prop=\"name\" />\n                    <el-table-column label=\"班级\" prop=\"className\" />\n                    <el-table-column\n                        label=\"专业\"\n                        min-width=\"150px\"\n                        prop=\"majorName\"\n                    />\n                    <el-table-column label=\"性别\" prop=\"sex\" width=\"80px\" />\n                    <el-table-column\n                        label=\"上次登录\"\n                        prop=\"lastLoginTime\"\n                        width=\"130px\"\n                    />\n                    <el-table-column\n                        align=\"center\"\n                        label=\"操作\"\n                        width=\"250px\"\n                        fixed=\"right\"\n                    >\n                        <template slot-scope=\"scope\">\n                            <el-button\n                                @click=\"edit(scope.row.id)\"\n                                type=\"success\"\n                                style=\"font-size: 18px\"\n                            >\n                                编辑\n                            </el-button>\n                            <el-button\n                                @click=\"deleteItem(scope.row.id)\"\n                                type=\"danger\"\n                                style=\"font-size: 18px\"\n                            >\n                                删除\n                            </el-button>\n                        </template>\n                    </el-table-column>\n                </el-table>\n            </div>\n\n            <el-row justify=\"center\" type=\"flex\" style=\"margin: 5px 0 5px 0\">\n                <el-pagination\n                    :current-page.sync=\"pageIndex\"\n                    :page-size=\"pageSize\"\n                    :total=\"pageSize * pageCount\"\n                    @current-change=\"getPage\"\n                    background\n                    layout=\"prev, pager, next\"\n                >\n                </el-pagination>\n            </el-row>\n\n            <el-dialog :visible.sync=\"editing\" title=\"编辑\" width=\"30%\">\n                <el-form :model=\"entityForm\" label-width=\"70px\" ref=\"form\">\n                    <el-form-item label=\"姓名\">\n                        <el-input v-model=\"entityForm.name\"></el-input>\n                    </el-form-item>\n                    <el-form-item label=\"学号\">\n        