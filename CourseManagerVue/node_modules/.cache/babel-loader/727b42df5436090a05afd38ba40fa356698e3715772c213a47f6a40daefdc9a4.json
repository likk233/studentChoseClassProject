{"ast":null,"code":"import * as api from \"../../api/admin/department\";\nexport default {\n  name: \"AdminDepartment\",\n  data() {\n    return {\n      queryForm: {\n        name: \"\"\n      },\n      entityForm: {},\n      tableData: [],\n      pageSize: api.pageSize,\n      pageCount: 1,\n      pageIndex: 1,\n      editing: false\n    };\n  },\n  methods: {\n    query() {\n      api.getPageCount(this.queryForm.name).then(res => {\n        this.pageCount = res;\n        this.pageIndex = 1;\n        this.getPage(1);\n      });\n    },\n    getPage(pageIndex) {\n      api.getPage(pageIndex, this.queryForm.name).then(res => {\n        this.tableData = res;\n      });\n    },\n    create() {\n      this.entityForm = {\n        id: -1,\n        name: \"\"\n      };\n      this.editing = true;\n    },\n    edit(id) {\n      api.get(id).then(res => {\n        this.entityForm = res;\n        this.editing = true;\n      });\n    },\n    save() {\n      if (this.entityForm.id === -1) {\n        api.create(this.entityForm).then(() => {\n          this.finishSave();\n        });\n      } else {\n        api.update(this.entityForm).then(() => {\n          this.finishSave();\n        });\n      }\n    },\n    finishSave() {\n      this.$message.success(\"成功\");\n      this.getPage(this.pageIndex);\n      this.editing = false;\n    },\n    deleteItem(id) {\n      api.deleteItem(id).then(() => {\n        this.$message.success(\"删除成功\");\n        this.getPage(this.pageIndex);\n      });\n    }\n  },\n  created() {\n    this.query();\n  }\n};","map":{"version":3,"names":["api","name","data","queryForm","entityForm","tableData","pageSize","pageCount","pageIndex","editing","methods","query","getPageCount","then","res","getPage","create","id","edit","get","save","finishSave","update","$message","success","deleteItem","created"],"sources":["/Users/lizi/Documents/GitHub/lib/studentChoseClassProject/CourseManagerVue/src/views/admin/AdminDepartment.vue"],"sourcesContent":["<template>\n    <div class=\"department-wrap\">\n        <div class=\"crumbs\">\n            <el-breadcrumb separator=\"/\">\n                <el-breadcrumb-item>\n                    <b style=\"font-size: 22px\"> 院系管理</b>\n                </el-breadcrumb-item>\n            </el-breadcrumb>\n        </div>\n\n        <div class=\"container\">\n            <div class=\"query-form\">\n                <el-row :gutter=\"20\">\n                    <el-col :span=\"2\">\n                        <el-button @click=\"create\" style=\"font-size: 18px\"\n                            > 创建</el-button\n                        >\n                    </el-col>\n                    <el-col :offset=\"13\" :span=\"6\">\n                        <el-input\n                            @keyup.enter.native=\"query\"\n                            placeholder=\"系名\"\n                            v-model=\"queryForm.name\"\n                        />\n                    </el-col>\n                    <el-col :span=\"3\">\n                        <el-button @click=\"query\" type=\"primary\" style=\"font-size: 18px;\">\n                             搜索\n                        </el-button>\n                    </el-col>\n                </el-row>\n            </div>\n\n            <div class=\"table\">\n                <el-table :data=\"tableData\" stripe>\n                    <el-table-column label=\"系Id\" prop=\"id\" />\n                    <el-table-column label=\"系名\" prop=\"name\" />\n                    <el-table-column label=\"专业数\" prop=\"majorCount\" />\n                    <el-table-column label=\"教师数\" prop=\"teacherCount\" />\n                    <el-table-column\n                        align=\"center\"\n                        label=\"操作\"\n                        width=\"250px\"\n                        fixed=\"right\"\n                    >\n                        <template slot-scope=\"scope\">\n                            <el-button\n                                @click=\"edit(scope.row.id)\"\n                                type=\"success\" style=\"font-size: 18px;\"\n                            > 编辑\n                            </el-button>\n                            <el-button\n                                @click=\"deleteItem(scope.row.id)\"\n                                type=\"danger\" style=\"font-size: 18px;\"\n                            > 删除\n                            </el-button>\n                        </template>\n                    </el-table-column>\n                </el-table>\n            </div>\n\n            <el-row justify=\"center\" type=\"flex\" style=\"margin: 5px 0 5px 0\">\n                <el-pagination\n                    :current-page.sync=\"pageIndex\"\n                    :page-size=\"pageSize\"\n                    :total=\"pageSize * pageCount\"\n                    @current-change=\"getPage\"\n                    background\n                    layout=\"prev, pager, next\"\n                >\n                </el-pagination>\n            </el-row>\n\n            <el-dialog :visible.sync=\"editing\" title=\"编辑\" width=\"30%\">\n                <el-form :model=\"entityForm\" label-width=\"70px\" ref=\"form\">\n                    <el-form-item label=\"系名\">\n                        <el-input v-model=\"entityForm.name\"></el-input>\n                    </el-form-item>\n                </el-form>\n                <span class=\"dialog-footer\" slot=\"footer\">\n                    <el-button\n                        @click=\"save\"\n                        type=\"primary\"\n                         style=\"font-size: 18px;\"\n                    >\n                        确 定</el-button\n                    >\n                    <el-button @click=\"editing = false\" style=\"font-size: 18px;\"> 取 消</el-button>\n                </span>\n            </el-dialog>\n        </div>\n    </div>\n</template>\n\n<script>\nimport * as api from \"../../api/admin/department\";\n\nexport default {\n    name: \"AdminDepartment\",\n    data() {\n        return {\n            queryForm: {\n                name: \"\",\n            },\n            entityForm: {},\n            tableData: [],\n            pageSize: api.pageSize,\n            pageCount: 1,\n            pageIndex: 1,\n            editing: false,\n        };\n    },\n    methods: {\n        query() {\n            api.getPageCount(this.queryForm.name).then((res) => {\n                this.pageCount = res;\n                this.pageIndex = 1;\n                this.getPage(1);\n            });\n        },\n        getPage(pageIndex) {\n            api.getPage(pageIndex, this.queryForm.name).then((res) => {\n                this.tableData = res;\n            });\n        },\n        create() {\n            this.entityForm = {\n                id: -1,\n                name: \"\",\n            };\n            this.editing = true;\n        },\n        edit(id) {\n            api.get(id).then((res) => {\n                this.entityForm = res;\n                this.editing = true;\n            });\n        },\n        save() {\n            if (this.entityForm.id === -1) {\n                api.create(this.entityForm).then(() => {\n                    this.finishSave();\n                });\n            } else {\n                api.update(this.entityForm).then(() => {\n                    this.finishSave();\n                });\n            }\n        },\n        finishSave() {\n            this.$message.success(\"成功\");\n            this.getPage(this.pageIndex);\n            this.editing = false;\n        },\n        deleteItem(id) {\n            api.deleteItem(id).then(() => {\n                this.$message.success(\"删除成功\");\n                this.getPage(this.pageIndex);\n            });\n        },\n    },\n    created() {\n        this.query();\n    },\n};\n</script>\n\n<style scoped></style>\n"],"mappings":"AA+FA,OAAO,KAAKA,GAAE,MAAO,4BAA4B;AAEjD,eAAe;EACXC,IAAI,EAAE,iBAAiB;EACvBC,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,SAAS,EAAE;QACPF,IAAI,