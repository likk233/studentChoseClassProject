=\"font-size: 18px\"\n                        >\n                            搜索\n                        </el-button>\n                    </el-col>\n                </el-row>\n            </div>\n\n            <div class=\"table\">\n                <el-table :data=\"tableData\" stripe>\n                    <el-table-column label=\"教师Id\" prop=\"id\" />\n                    <el-table-column label=\"教师工号\" prop=\"number\" />\n                    <el-table-column label=\"教师姓名\" prop=\"name\" />\n                    <el-table-column label=\"所属系\" prop=\"departmentName\" />\n                    <el-table-column\n                        align=\"center\"\n                        label=\"操作\"\n                        width=\"250px\"\n                        fixed=\"right\"\n                    >\n                        <template slot-scope=\"scope\">\n                            <el-button\n                                @click=\"edit(scope.row.id)\"\n                                type=\"success\"\n                                style=\"font-size: 18px\"\n                            >\n                                编辑\n                            </el-button>\n                            <el-button\n                                @click=\"deleteItem(scope.row.id)\"\n                                style=\"font-size: 18px\"\n                                type=\"danger\"\n                            >\n                                删除\n                            </el-button>\n                        </template>\n                    </el-table-column>\n                </el-table>\n            </div>\n\n            <el-row justify=\"center\" type=\"flex\" style=\"margin: 5px 0 5px 0\">\n                <el-pagination\n                    :current-page.sync=\"pageIndex\"\n                    :page-size=\"pageSize\"\n                    :total=\"pageSize * pageCount\"\n                    @current-change=\"getPage\"\n                    background\n                 