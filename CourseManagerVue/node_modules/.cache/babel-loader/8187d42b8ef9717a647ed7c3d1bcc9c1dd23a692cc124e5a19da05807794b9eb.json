{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport * as api from \"../../api/admin/admin\";\nimport Permission from \"../../common/permission\";\nexport default {\n  name: \"AdminAdmin\",\n  data() {\n    return {\n      entityForm: {},\n      priGroup: [],\n      tableData: [],\n      editing: false\n    };\n  },\n  methods: {\n    getList() {\n      api.list().then(res => {\n        res = this.addPermissionProp(res);\n        this.tableData = res;\n      });\n    },\n    addPermissionProp(data) {\n      for (let i = 0; i < data.length; i++) {\n        let count = 0;\n        for (let bit = 0; bit < Permission.permissionCount; bit++) {\n          if ((1 << bit & data[i].privilege) === 1 << bit) {\n            count++;\n          }\n        }\n        data[i].permission = count;\n      }\n      return data;\n    },\n    create() {\n      this.priGroup = [];\n      this.entityForm = {\n        id: -1,\n        username: \"\",\n        privilege: \"\",\n        password: \"\"\n      };\n      this.editing = true;\n    },\n    edit(id) {\n      api.get(id).then(res => {\n        this.setPriGroup(res);\n        this.entityForm = res;\n        this.editing = true;\n      });\n    },\n    setPriGroup(data) {\n      let group = [];\n      for (let bit = 0; bit < Permission.permissionCount; bit++) {\n        if ((1 << bit & data.privilege) === 1 << bit) {\n          group.push((1 << bit).toString());\n        }\n      }\n      this.priGroup = group;\n    },\n    readPrivilegeFromGroup() {\n      let privilege = 0;\n      for (let i = 0; i < this.priGroup.length; i++) {\n        privilege += parseInt(this.priGroup[i]);\n      }\n      return privilege;\n    },\n    save() {\n      this.entityForm.privilege = this.readPrivilegeFromGroup();\n      if (this.entityForm.id === -1) {\n        api.create(this.entityForm).then(() => {\n          this.finishSave();\n        });\n      } else {\n        api.update(this.entityForm).then(() => {\n          this.finishSave();\n        });\n      }\n    },\n    finishSave() {\n      this.$message.success(\"成功\");\n      this.getList();\n      this.editing = false;\n    },\n    deleteItem(id) {\n      api.deleteItem(id).then(() => {\n        this.$message.success(\"删除成功\");\n        this.getList();\n      });\n    }\n  },\n  created() {\n    this.getList();\n  }\n};","map":{"version":3,"names":["api","Permission","name","data","entityForm","priGroup","tableData","editing","methods","getList","list","then","res","addPermissionProp","i","length","count","bit","permissionCount","privilege","permission","create","id","username","password","edit","get","setPriGroup","group","push","toString","readPrivilegeFromGroup","parseInt","save","finishSave","update","$message","success","deleteItem","created"],"sources":["/Users/lizi/Documents/GitHub/lib/studentChoseClassProject/CourseManagerVue/src/views/admin/AdminAdmin.vue"],"sourcesContent":["<template>\n    <div class=\"admin-wrap\">\n        <div class=\"crumbs\">\n            <el-breadcrumb separator=\"/\">\n                <el-breadcrumb-item>\n                    <b style=\"font-size: 22px\"> 管理员管理</b>\n                </el-breadcrumb-item>\n            </el-breadcrumb>\n        </div>\n\n        <div class=\"container\">\n            <div class=\"query-form\">\n                <el-row :gutter=\"20\">\n                    <el-col :span=\"2\">\n                        <el-button @click=\"create\" style=\"font-size: 18px\"\n                            >\n                            创建</el-button\n                        >\n                    </el-col>\n                </el-row>\n            </div>\n\n            <div class=\"table\">\n                <el-table :data=\"tableData\" stripe>\n                    <el-table-column label=\"管理员Id\" prop=\"id\" />\n                    <el-table-column label=\"管理员用户名\" prop=\"username\" />\n                    <el-table-column label=\"权限个数\" prop=\"permission\" />\n                    <el-table-c