     <div class=\"table\">\n                <el-table :data=\"tableData\" stripe>\n                    <el-table-column label=\"课程Id\" prop=\"id\" />\n                    <el-table-column label=\"课程名\" prop=\"name\" width=\"200px\" />\n                    <el-table-column label=\"教师\" prop=\"teacherName\" />\n                    <el-table-column label=\"所属系\" prop=\"departmentName\" />\n                    <el-table-column label=\"年级\" prop=\"grade\" />\n                    <el-table-column label=\"学分\" prop=\"credit\" />\n                    <el-table-column\n                        align=\"center\"\n                        label=\"上课时间\"\n                        prop=\"time\"\n                        width=\"130px\"\n                    />\n                    <el-table-column label=\"已选人数\" prop=\"selectedCount\" />\n                    <el-table-column label=\"最大容量\" prop=\"maxSize\" />\n                    <el-table-column\n                        align=\"center\"\n                        label=\"操作\"\n                        width=\"250px\"\n                        fixed=\"right\"\n                    >\n                        <template slot-scope=\"scope\">\n                            <el-button\n                                @click=\"edit(scope.row.id)\"\n                                type=\"success\"\n                                style=\"font-size: 18px\"\n                            >\n                                编辑\n                            </el-button>\n                            <el-button\n                                @click=\"deleteItem(scope.row.id)\"\n                                type=\"danger\"\n                                style=\"font-size: 18px\"\n                            >\n                                删除\n                            </el-button>\n                        </template>\n                    </el-table-column>\n                </el-table>\n            </div>\n\n            <el-row justify=\"center\" type=\"flex\" style=\"margin: 5px 0 5px 0\">\n                <el-pagination\n                    :current-page.sync=\"pageIndex\"\n                    :page-size=\"pageSize\"\n                    :total=\"pageSize * pageCount\"\n                    @current-change=\"getPage\"\n                    background\n                    layout=\"prev, pager, next\"\n                >\n                </el-pagination>\n            </el-row>\n\n            <el-dialog :visible.sync=\"editing\" title=\"编辑\" width=\"30%\">\n                <el-form :model=\"entityForm\" label-width=\"70px\" ref=\"form\">\n                    <el-form-item label=\"课程名\">\n                        <el-input v-model=\"entityForm.name\"></el-input>\n                    </el-form-item>\n                    <el-form-item label=\"授课教师\">\n                        <el-select\n                            placeholder=\"请选择教师\"\n                            v-model=\"entityForm.teacherId\"\n                        >\n                            <el-option\n                                :key=\"index\"\n                                :label=\"item.name\"\n                                :value=\"item.id\"\n                                v-for=\"(item, index) in teachers\"\n                            />\n                        </el-select>\n                    </el-form-item>\n                    <el-form-item label=\"年级\">\n                        <el-input\n                            type=\"number\"\n                            v-model=\"entityForm.grade\"\n                        ></el-input>\n                    </el-form-item>\n                    <el-form-item label=\"上课时间\">\n                        <el-select v-model=\"courseDay\">\n                            <el-option\n                                :key=\"index\"\n                                :label=\"item\"\n                                :value=\"index\"\n                                v-for=\"(item, index) in days\"\n                  