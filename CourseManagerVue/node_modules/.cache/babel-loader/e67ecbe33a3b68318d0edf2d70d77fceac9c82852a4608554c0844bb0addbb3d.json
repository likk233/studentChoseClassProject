rr.push({\n            courseName: \"\",\n            teacherName: \"\",\n            location: \"\"\n          });\n        }\n        table.push(arr);\n      }\n      this.tableData = table;\n    },\n    digital2Chinese(num, identifier) {\n      const character = [\n        \"零\",\n        \"一\",\n        \"二\",\n        \"三\",\n        \"四\",\n        \"五\",\n        \"六\",\n        \"七\",\n        \"八\",\n        \"九\",\n        \"十\"\n      ];\n      return identifier === \"week\" && num === 7 ? \"日\" : character[num];\n    }\n  },\n  created() {\n    this.clearTable();\n  }\n};\n</script>\n\n<style lang=\"scss\" scoped>\ntable {\n  table-layout: fixed;\n  width: 100%;\n\n  thead {\n    background-color: #3CB371;\n    th {\n      color: #fff;\n      line-height: 25px;\n      font-weight: normal;\n    }\n  }\n\n  tbody {\n    background-color: #90EE90;\n\n    td {\n      line-height: 0;\n      color: black;\n      height: 50px;\n      padding-top: 5px;\n\n      span {\n        line-height: 0.9;\n      }\n    }\n\n    .course-name {\n      color: #000;\n      font-size: 16px;\n      font-weight: 200;\n    }\n  }\n\n  th,\n  td {\n    width: 60px;\n    padding: 2px 2px;\n    font-size: 12px;\n    text-align: center;\n  }\n\n  tr td:first-child {\n    color: #333;\n\n    .period {\n      font-size: 8px;\n    }\n  }\n}\n</style>\n"],"mappings":";AAkCA,eAAe;EACbA,IAAI,EAAE,WAAW;EACjBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,SAAS,EAAE,CACT,CACE;QACEC,UAAU,EAAE,EAAE;QACdC,WAAW,EAAE,EAAE;QACfC,QAAQ,EAAE;MACZ,EACF;IAEJ,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,UAAUA,CAACN,IAAI,EAAE;MACf,IAAI,CAACO,UAAU,CAAC,CAAC;MACjB,KAAK,IAAIC,CAAA,GAAI,CAAC,EAAEA,CAAA,GAAIR,IAAI,CAACS,MAAM,EAAED,CAAC,EAAE,EAAE;QACpC,IAAI,CAACE,SAAS,CAACV,IAAI,CAACQ,CAAC,CAAC,CAAC;MACzB;IACF,CAAC;IACDE,SAASA,CAACC,IAAI,EAAE;MACd,IAAIC,KAAI,GAAID,IAAI,CAACE,IAAI,CAACD,KAAK,CAAC,GAAG,CAAC;MAChC,IAAIE,IAAG,GAAIC,QAAQ,CAACH,KAAK,CAAC,CAAC,CAAC,CAAC;MAC7B,IAAII,MAAK,GAAID,QAAQ,CAACH,KAAK,CAAC,CAAC,CAAC,CAAC;MAC/B,IAAIH,