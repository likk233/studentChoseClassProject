{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { getLoginStatus } from \"../api/user\";\nimport SideBar from \"../components/SideBar\";\nimport HeadBar from \"../components/HeadBar\";\nimport UserType from \"../common/userType\";\nexport default {\n  name: \"Container\",\n  components: {\n    HeadBar,\n    SideBar\n  },\n  methods: {\n    redirectHome(userType) {\n      if (userType === UserType.student) {\n        this.$router.push({\n          name: \"student-home\"\n        });\n      } else if (userType === UserType.teacher) {\n        this.$router.push({\n          name: \"teacher-home\"\n        });\n      } else if (userType === UserType.admin) {\n        this.$router.push({\n          name: \"admin-home\"\n        });\n      }\n    }\n  },\n  created() {\n    getLoginStatus().then(res => {\n      this.$store.commit(\"login\", res);\n      if (!res.loggedIn) {\n        this.$router.push({\n          name: \"login\"\n        });\n      } else if (this.$route.path === \"/\") {\n        this.redirectHome(res.userType);\n      }\n    }).catch(error => {\n      this.$message({\n        showClose: true,\n        message: error,\n        type: \"error\",\n        duration: 80000\n      });\n    });\n  }\n};","map":{"version":3,"names":["getLoginStatus","SideBar","HeadBar","UserType","name","components","methods","redirectHome","userType","student","$router","push","teacher","admin","created","then","res","$store","commit","loggedIn","$route","path","catch","error","$message","showClose","message","type","duration"],"sources":["/Users/lizi/Documents/GitHub/lib/studentChoseClassProject/CourseManagerVue/src/views/Container.vue"],"sourcesContent":["<!--\n * @Description: \n * @Author: Rabbiter\n * @Date: 2023-06-06 09:09:08\n-->\n<template>\n    <div class=\"home\">\n        <head-bar />\n        <side-bar />\n\n        <div class=\"content-box\">\n            <div class=\"content\" v-loading=\"this.$store.state.loading\">\n                <router-view />\n                <el-backtop target=\".content\"></el-backtop>\n            </div>\n        </div>\n    </div>\n</template>\n\n<script>\nimport { getLoginStatus } from \"../api/user\";\nimport SideBar from \"../components/SideBar\";\nimport HeadBar from \"../components/HeadBar\";\nimport UserType from \"../common/userType\";\n\nexport default {\n    name: \"Container\",\n    components: {\n        HeadBar,\n        SideBar,\n    },\n    methods: {\n        redirectHome(userType) {\n            if (userType === UserType.student) {\n                this.$router.push({ name: \"student-home\" });\n            } else if (userType === UserType.teacher) {\n                this.$router.push({ name: \"teacher-home\" });\n            } else if (userType === UserType.admin) {\n                this.$router.push({ name: \"admin-home\" });\n            }\n        },\n    },\n    created() {\n        getLoginStatus()\n            .then((res) => {\n                this.$store.commit(\"login\", res);\n                if (!res.loggedIn) {\n                    this.$router.push({ name: \"login\" });\n                } else if (this.$route.path === \"/\") {\n                    this.redirectHome(res.userType);\n                }\n            })\n            .catch((error) => {\n                this.$message({\n                    showClose: true,\n                    message: error,\n                    type: \"error\",\n                    duration: 80000,\n                });\n            });\n    },\n};\n</script>\n\n<style scoped>\n.content-box {\n    position: absolute;\n    left: 200px;\n    right: 0;\n    top: 70px;\n    bottom: 0;\n    background: #f0f0f0;\n}\n\n.content {\n    width: auto;\n    height: 100%;\n    padding: 10px;\n    overflow-y: scroll;\n    box-sizing: border-box;\n}\n</style>\n"],"mappings":";AAoBA,SAASA,cAAa,QAAS,aAAa;AAC5C,OAAOC,OAAM,MAAO,uBAAuB;AAC3C,OAAOC,OAAM,MAAO,uBAAuB;AAC3C,OAAOC,QAAO,MAAO,oBAAoB;AAEzC,eAAe;EACXC,IAAI,EAAE,WAAW;EACjBC,UAAU